<?xml version="1.0" encoding="UTF-8"?>

<!-- **********IMPORTANT**********To Generate HTML Extent Report always execute test cases from 
TestNG XML file mandatory*******IMPORTANT********** -->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="FREE CRM Application Test Automation Test Suite">

	<listeners>
		<listener class-name="com.qa.ExtentReportGenerate.ExtentReport" />
	</listeners>


	<test name="Free CRM App Test Cases">
		<classes>
			<class name="com.crm.qa.testcases.LoginTestcase"></class>
			<class name="com.crm.qa.testcases.HomeTestcase"></class>
			
			

		</classes>
	</test>
</suite>

